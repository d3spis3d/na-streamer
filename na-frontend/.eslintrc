{
  "extends": "airbnb",
  "env": {
    "browser": true,
  },
  "rules" : {
      "react/jsx-filename-extension": 0,
      "comma-dangle": 0,
      /*will re-enable below after some guidance from J.Tillman himself*/
      "react/prop-types": 0,
      "react/prefer-stateless-function": 0,
      "react/jsx-no-bind": 0,
      "react/no-multi-comp": 0
    }
}
